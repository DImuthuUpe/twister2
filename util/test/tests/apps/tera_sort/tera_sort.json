{
  "id": "terasort_bench_base",
  "directory": {
    "relativeToRoot": true,
    "path": ""
  },
  "jar": "libexamples-java.jar",
  "className": "edu.iu.dsc.tws.examples.task.batch.sort.TeraSort",
  "resultsFile": "tera_sort.csv",
  "args": [
    {
      "id": "size",
      "name": "Data Size",
      "optional": false,
      "values": {
        "type": "steps",
        "from": 1,
        "to": 128,
        "step": 4
      }
    },
    {
      "id": "keyRange",
      "name": "Range of Keys",
      "optional": false,
      "values": {
        "type": "fixed",
        "value": 1000000
      }
    },
    {
      "id": "valueSize",
      "name": "Size of Value",
      "optional": false,
      "values": {
        "type": "fixed",
        "value": 100
      }
    },
    {
      "id": "sources",
      "name": "No of Sources",
      "optional": false,
      "values": {
        "type": "fixed",
        "value": 128
      }
    },
    {
      "id": "instances",
      "name": "Workers",
      "optional": false,
      "values": {
        "type": "fixed",
        "value": 128
      }
    },
    {
      "id": "instanceCPUs",
      "name": "cpus",
      "optional": false,
      "values": {
        "type": "fixed",
        "value": 1
      }
    },
    {
      "id": "instanceMemory",
      "name": "Memory",
      "optional": false,
      "values": {
        "type": "fixed",
        "value": 1
      }
    }
  ]
}